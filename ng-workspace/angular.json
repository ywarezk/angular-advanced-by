{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "rxjs-tutorial": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/rxjs-tutorial",
      "sourceRoot": "projects/rxjs-tutorial/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/rxjs-tutorial",
            "index": "projects/rxjs-tutorial/src/index.html",
            "main": "projects/rxjs-tutorial/src/main.ts",
            "polyfills": "projects/rxjs-tutorial/src/polyfills.ts",
            "tsConfig": "projects/rxjs-tutorial/tsconfig.app.json",
            "aot": false,
            "assets": [
              "projects/rxjs-tutorial/src/favicon.ico",
              "projects/rxjs-tutorial/src/assets"
            ],
            "styles": [
              "projects/rxjs-tutorial/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/rxjs-tutorial/src/environments/environment.ts",
                  "with": "projects/rxjs-tutorial/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "rxjs-tutorial:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "rxjs-tutorial:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "rxjs-tutorial:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/rxjs-tutorial/src/test.ts",
            "polyfills": "projects/rxjs-tutorial/src/polyfills.ts",
            "tsConfig": "projects/rxjs-tutorial/tsconfig.spec.json",
            "karmaConfig": "projects/rxjs-tutorial/karma.conf.js",
            "assets": [
              "projects/rxjs-tutorial/src/favicon.ico",
              "projects/rxjs-tutorial/src/assets"
            ],
            "styles": [
              "projects/rxjs-tutorial/src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/rxjs-tutorial/tsconfig.app.json",
              "projects/rxjs-tutorial/tsconfig.spec.json",
              "projects/rxjs-tutorial/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/rxjs-tutorial/e2e/protractor.conf.js",
            "devServerTarget": "rxjs-tutorial:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "rxjs-tutorial:serve:production"
            }
          }
        }
      }
    },
